{
    "name": "rotrust-network",
    "version": "1.0.0",
    "client": {
        "organization": "NotaryOrg",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                },
                "orderer": "300"
            }
        }
    },
    "channels": {
        "rotrust-channel": {
            "orderers": [
                "orderer.rotrust.ro"
            ],
            "peers": {
                "peer0.notary.rotrust.ro": {
                    "endorsingPeer": true,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                },
                "peer0.landregistry.rotrust.ro": {
                    "endorsingPeer": true,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                },
                "peer0.bank.rotrust.ro": {
                    "endorsingPeer": true,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                }
            }
        }
    },
    "organizations": {
        "NotaryOrg": {
            "mspid": "NotaryMSP",
            "peers": [
                "peer0.notary.rotrust.ro"
            ],
            "certificateAuthorities": [
                "ca.notary.rotrust.ro"
            ]
        },
        "LandRegistryOrg": {
            "mspid": "LandRegistryMSP",
            "peers": [
                "peer0.landregistry.rotrust.ro"
            ],
            "certificateAuthorities": [
                "ca.landregistry.rotrust.ro"
            ]
        },
        "BankOrg": {
            "mspid": "BankMSP",
            "peers": [
                "peer0.bank.rotrust.ro"
            ],
            "certificateAuthorities": [
                "ca.bank.rotrust.ro"
            ]
        },
        "OrdererOrg": {
            "mspid": "OrdererMSP",
            "orderers": [
                "orderer.rotrust.ro"
            ]
        }
    },
    "orderers": {
        "orderer.rotrust.ro": {
            "url": "grpcs://orderer.rotrust.ro:7050",
            "grpcOptions": {
                "ssl-target-name-override": "orderer.rotrust.ro",
                "hostnameOverride": "orderer.rotrust.ro"
            },
            "tlsCACerts": {
                "path": "crypto-config/ordererOrganizations/rotrust.ro/orderers/orderer.rotrust.ro/tls/ca.crt"
            }
        }
    },
    "peers": {
        "peer0.notary.rotrust.ro": {
            "url": "grpcs://peer0.notary.rotrust.ro:7051",
            "grpcOptions": {
                "ssl-target-name-override": "peer0.notary.rotrust.ro",
                "hostnameOverride": "peer0.notary.rotrust.ro"
            },
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/notary.rotrust.ro/peers/peer0.notary.rotrust.ro/tls/ca.crt"
            }
        },
        "peer0.landregistry.rotrust.ro": {
            "url": "grpcs://peer0.landregistry.rotrust.ro:7051",
            "grpcOptions": {
                "ssl-target-name-override": "peer0.landregistry.rotrust.ro",
                "hostnameOverride": "peer0.landregistry.rotrust.ro"
            },
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/landregistry.rotrust.ro/peers/peer0.landregistry.rotrust.ro/tls/ca.crt"
            }
        },
        "peer0.bank.rotrust.ro": {
            "url": "grpcs://peer0.bank.rotrust.ro:7051",
            "grpcOptions": {
                "ssl-target-name-override": "peer0.bank.rotrust.ro",
                "hostnameOverride": "peer0.bank.rotrust.ro"
            },
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/bank.rotrust.ro/peers/peer0.bank.rotrust.ro/tls/ca.crt"
            }
        }
    },
    "certificateAuthorities": {
        "ca.notary.rotrust.ro": {
            "url": "https://ca.notary.rotrust.ro:7054",
            "caName": "ca.notary.rotrust.ro",
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/notary.rotrust.ro/ca/ca.notary.rotrust.ro-cert.pem"
            },
            "httpOptions": {
                "verify": false
            }
        },
        "ca.landregistry.rotrust.ro": {
            "url": "https://ca.landregistry.rotrust.ro:7054",
            "caName": "ca.landregistry.rotrust.ro",
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/landregistry.rotrust.ro/ca/ca.landregistry.rotrust.ro-cert.pem"
            },
            "httpOptions": {
                "verify": false
            }
        },
        "ca.bank.rotrust.ro": {
            "url": "https://ca.bank.rotrust.ro:7054",
            "caName": "ca.bank.rotrust.ro",
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/bank.rotrust.ro/ca/ca.bank.rotrust.ro-cert.pem"
            },
            "httpOptions": {
                "verify": false
            }
        }
    }
}